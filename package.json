{
  "name": "movile-messaging",
  "version": "1.2.1-rc.1",
  "description": "Simple node.js wrapper for Movile's Messaging API",
  "repository": "vspedr/movile-messaging",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "precommit": "yarn lint",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk protect",
    "test": "jest --coverage"
  },
  "keywords": [
    "movile",
    "messaging",
    "send",
    "sms"
  ],
  "author": "vspedr",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "qs": "^6.4.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^1.0.0",
    "jest": "^22.0.0",
    "snyk": "^1.30.1"
  },
  "snyk": true,
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/test/**",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  }
}
